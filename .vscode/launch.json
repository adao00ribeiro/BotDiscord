{
    "configurations": [
    {
        "name": "Launch Program",
        "program": "${workspaceFolder}/index.js",
        "request": "launch",
        "skipFiles": [
            "<node_internals>/**"
        ],
        "type": "pwa-node",
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**" // this lines prevent reading source maps from node_modules
      ]
    }
    ]
}